    <div class="res-search-response-header"><span>Тендеры(<% print(count) %>)</span><a href="">Показать все найденные Тендерых</a></div>
    <ul>
        <li>
            <div><span>Название:</span></div>
            <div><span>от компании:</span></div>
        </li>

        <% _.each( items, function(item){ %>
            <li class= 'res-search-tender-element' tender_id="<% print(item.id)%>">
                <div><span><% print(item.name) %></span></div>
                <div><span><% print(item.company) %></span></div>
            </li>
        <% }) %>
    </ul>
